<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dunno">
    <!-- Firebase permissions (if needed) -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Dunno">
        <activity
            android:name=".model.HomeActivity"
            android:exported="false" />
        <!-- MainActivity entry -->
        <activity
            android:name=".model.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- SecondActLogIn entry -->
        <activity android:name=".model.SecondActLogIn" /> <!-- SecondActSignUp entry -->
        <activity android:name=".model.SecondActSignUp" />
    </application>

</manifest>